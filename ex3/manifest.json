{
  "name": "popupとcontent_scriptsの連携",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_title": "popupとcontent_scriptsの連携",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "js": ["jquery.min.js", "content_script.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ]
}
